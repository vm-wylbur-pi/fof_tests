/*  Example of a dithered luminance curve (4 bits in this case) with FastLED 3.0

    Dither concept and code: https://github.com/raplin/HexaWS2811
    Luminance idea: http://ledshield.wordpress.com/2012/11/13/led-brightness-to-your-eye-gamma-correction-no/
    LUminance code: http://forum.arduino.cc/index.php/topic,147810.0.html

    FastLED community: https://plus.google.com/communities/109127054924227823508

    kasperkamperman.com, 28-10-2014
	https://gist.githubusercontent.com/kasperkamperman/4461a70dd0841f80ddf4/raw/72e4f35f9b9da786eaa14dcc7ee2fe2320dd73a3/dithering_example.ino

*/

#include "FastLED.h"

extern const uint8_t luminanceLUT[];

#define NUM_LEDS 30
#define DITHER_STEPS 16

int fadeMaximum = 255; // make it 255 for full range

CRGB leds[NUM_LEDS];

unsigned long t1; // for measuring purposes
unsigned long t2; // for measuring purposes
unsigned long t3 = 0; // for measuring purposes

uint8_t ditherFrame = 0;
const uint8_t ditherMaxSteps = DITHER_STEPS - 1;

unsigned long previousMicros  = 0;
unsigned long currentMicros   = 0;

unsigned long previousMillis  = 0;
unsigned long currentMillis   = 0;

const int interval = 1000/40;

int fadeVal = 0;
int fadeSpeed = 1;

void setup() {

  //FastLED.addLeds<WS2812B, 2, GRB>(leds, NUM_LEDS);
  FastLED.addLeds<APA102, 11, 13, BGR, DATA_RATE_MHZ(12)>(leds, NUM_LEDS);
  FastLED.setDither(0); // turn off FastLED dithering

  Serial.begin(57600);

  memset(leds, 0,  NUM_LEDS * sizeof(struct CRGB));
}

void loop()
{ // fixed framerate

  currentMicros = micros();

  // pwm APA102 x  580 hz > 1724us WS2812 around 400Hz > 2500us
  if (currentMicros - previousMicros >= 1724 == true ) {

    //setTime();

    ditherFrame++;
    if(ditherFrame>ditherMaxSteps) ditherFrame = 0;

    const uint8_t *luminanceLUTDithered = luminanceLUT+(ditherFrame<<8);

    for(int i = 0; i < NUM_LEDS; i++)
    {
       // yellow R and G
       leds[i] = CRGB(fadeVal,fadeVal,0);

       leds[i].r = pgm_read_byte_near(luminanceLUTDithered + leds[i].r);
       leds[i].g = pgm_read_byte_near(luminanceLUTDithered + leds[i].g);
       leds[i].b = pgm_read_byte_near(luminanceLUTDithered + leds[i].b);
    }

    //printTime();

    FastLED.show();

    //  fading we call this at 25fps

    previousMicros = currentMicros;

    currentMillis = millis();

    if (currentMillis - previousMillis >= interval == true ) {

      fadeVal = fadeVal + fadeSpeed;                 // change fadeVal by speed
      fadeVal = constrain(fadeVal, 0, fadeMaximum);  // keep fadeVal between 0 and maximum

      if(fadeVal==fadeMaximum || fadeVal==0)         // change from up>down or down-up (negative/positive)
      { fadeSpeed = -fadeSpeed;
      }

      previousMillis = currentMillis;
    }

  }

}

void setTime()
{   t1 = micros();
}

void printTime()
{
   t2 = micros()-t1;
   if(t2>t3) t3 = t2;

   Serial.print(F("update time: "));
   Serial.print(t3);
   Serial.print(" ");
   Serial.println(t2);

}

